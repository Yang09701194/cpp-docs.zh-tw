{
  "build": {
    "markdownEngineName": "markdig",
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "group": "group-vc2015",
        "src": ".",
        "exclude": [
          "**/obj/**",
          "vcppdocs/**",
          "**/includes/**"
        ]
      }
        ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "group": "group-vc2015",
        "src": ".",
        "exclude": [
          "**/obj/**",
          "vcppdocs/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
            "bilingual_type": "hover over",      
            "breadcrumb_path": "~/_breadcrumb/toc.yml",
            "ROBOTS": "INDEX,FOLLOW",
            "manager": "wpickett",
            "ms.date": "11/16/2016",
            "ms.topic": "conceptual",
            "audience": "developer",
            "ms.prod": "visual-cpp",
            "ms.tgt_pltfrm": "",
            "searchScope": ["C++"],
            "feedback_system": "GitHub",
            "feedback_github_repo": "MicrosoftDocs/cpp-docs.zh-tw",
            "feedback_product_url": "https://developercommunity.visualstudio.com/spaces/62/index.html"
    },
    "fileMetadata": {
      "author": {
        "standard-library/**/**.md": "corob-msft"
      },
      "ms.author": {
        "standard-library/**/**.md": "corob"
      }      
    },
    "template": [],
    "xref": ["msdn.4.5.2.zip", "namespaces.4.5.2.zip", "vs.110.zip", "vs.140.zip", "office15.zip"],
    "dest": "vcppdocs",
    "groups": {
        "group-vc2015": {
        "moniker_range": ">= vs-2015",
        "dest": "vcppdocs-2015"
      }
    }
  }
}
